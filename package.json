{
  "name": "meeting",
  "version": "1.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "engines": {
    "node": "8.11.1"
  },
  "dependencies": {
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "btoa": "^1.2.1",
    "excel4node": "^1.7.0",
    "express": "^4.16.4",
    "file-saver": "^1.3.8",
    "hummus": "^1.0.95",
    "hummus-recipe": "^1.8.0",
    "moment": "^2.23.0",
    "mysql": "^2.16.0",
    "node-mysql": "^0.4.2",
    "nodemon": "^1.18.9",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0"
  },
  "devDependencies": {
    "husky": "^1.2.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.15.3"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon --inspect server/index.js",
    "heroku-postbuild": "cd client && npm i && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
